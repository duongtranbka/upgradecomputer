#include <iostream>
using namespace std;
int n;
long a[1000];
long long tong,change;
int main()
{
//	freopen("input.txt","r",stdin);
	int t;
	cin>>t;
	for(int p=1;p<=t;p++)
	{
		cin >>n;
		tong=0;
		change=0;
		for(int i=0;i<n;i++)
		{
			cin>>a[i];
			tong=tong+a[i];
		}
		change=tong;
		for(int i=0;i<n-1;i++)
		{
			for(int j=i+1;j<n;j++)
			{
				if(a[i]<a[j])
				{
					int temp=a[j];
					a[j]=a[i];
					a[i]=temp;
				}
			}
		}
		if(n>2)
		{
		for(int i=0;i<n-2;i++)
		{
			change=change-a[i];
			tong=tong+change;
		}
		}
		cout<<"Case #"<<p<<endl;
		cout<<tong<<endl;
	}
	return 0;
}
